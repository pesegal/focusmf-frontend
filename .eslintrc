{
  "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 4
        }
      ],
      "semi": ["error", "never"],
      "quotes": ["error", "single"]
    }
}
