{
  "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 4
        }
      ]
    },
    "parserOptions": {
      "parser": "typescript-eslint-parser"
    }
}
