{
  "name": "focusmf-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "test:e2e": "cypress open"
  },
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "axios": "^0.18.0",
    "graphql": "^14.2.1",
    "vue": "^2.5.17",
    "vue-apollo": "^3.0.0-beta.28",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.0",
    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/node": "^12.0.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "cypress": "^3.3.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "graphql-tag": "^2.9.0",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-class-component": "^7.0.2",
    "vue-cli-plugin-apollo": "^0.20.0",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-property-decorator": "^8.1.0",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 4
        }
      ]
    }
  },
  "eslintIgnore": [
    "*.gql"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
